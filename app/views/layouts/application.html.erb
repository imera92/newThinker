<!DOCTYPE html>
<html>
  <head>
    <title>NewThinker</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<% if logged_in? %>
  		<%= link_to 'Log out', logout_path, method: :delete, data: { confirm: 'Esta seguro?' } %>
	    <%= yield %>
	<% else %>
		<%= yield %>
	<% end %>
  </body>
</html>